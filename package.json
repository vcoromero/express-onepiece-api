{
  "name": "express-onepiece-api",
  "version": "1.0.0",
  "description": "One Piece themed RESTful API built with Express.js",
  "main": "src/index.js",
  "scripts": {
    "start": "node -r dotenv/config src/index.js dotenv_config_path=configs/.env",
    "dev": "nodemon -r dotenv/config src/index.js dotenv_config_path=configs/.env",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "api",
    "express",
    "onepiece",
    "rest",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.911.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "sequelize": "^6.37.7",
    "serverless-http": "^4.0.0",
    "validator": "^13.15.15",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^14.4.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/config/**/*.js",
      "!src/utils/logger.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "overrides": {
    "validator": "^13.15.15"
  }
}
